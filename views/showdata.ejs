<%- include('components/head'); -%>
<%- include('components/navbar'); -%>

<div class="container my-4" style="margin-top:100px">

    <center>
        <table class="table table-striped table-primary">
            <thead>
              <tr>
                <th>ID</th>
                <th>Message</th>
                <th>Date</th>
              </tr>
            </thead>
            <tbody>
              <%if (data.length >0){ %>
              <% data.forEach(function(row){ %>
              <tr>
                <td><%= row.id %></td>
                <td><%= row.message %></td>
                <td><%= row.date %></td>
              </tr>
              <% })}else{ %>
                <tr>
                    <td colspan="6">No Record Found</td>
                    
                  </tr>
                <% } %>
            </tbody>
          </table>
     
    </center>

